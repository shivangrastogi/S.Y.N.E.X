{
  "browser": "chrome",
  "browser_profile": "Default",
  "browser_user_data_dir": "",
  "headless": false,
  "default_quality": "720p",
  "default_speed": 1.25,
  "default_volume": 0.8,
  "auto_play": true,
  "auto_fullscreen": false,
  "auto_theater_mode": false,
  "auto_captions": false,
  "search_timeout": 20,
  "max_search_results": 10,
  "filter_shorts": false,
  "filter_live": false,
  "prefer_official": true,
  "player_load_timeout": 15,
  "player_ready_timeout": 10,
  "video_element_timeout": 5,
  "seek_step_seconds": 10,
  "volume_step": 0.1,
  "speed_step": 0.25,
  "retry_enabled": true,
  "max_retries": 2,
  "retry_delay": 3,
  "retry_backoff_multiplier": 2.0,
  "reuse_session": true,
  "session_timeout": 3600,
  "auto_close_on_error": false,
  "recreate_on_crash": true,
  "query_min_length": 1,
  "query_max_length": 200,
  "normalize_queries": true,
  "remove_noise_words": true,
  "noise_words": [
    "play",
    "youtube",
    "on youtube",
    "search"
  ],
  "enable_history": false,
  "history_file": "youtube_history.json",
  "enable_queue": false,
  "queue_max_size": 50,
  "enable_recommendations": false,
  "lazy_load_player": true,
  "cache_session": true,
  "parallel_operations": false,
  "global_timeout": 120,
  "page_load_timeout": 15,
  "script_timeout": 10,
  "screenshot_on_error": false,
  "save_failed_queries": true,
  "failed_queries_file": "failed_youtube_queries.json",
  "fallback_to_native": true,
  "debug_mode": true,
  "verbose_logging": false,
  "log_search_queries": false,
  "log_player_actions": false,
  "support_hinglish": true,
  "support_unicode": true
}